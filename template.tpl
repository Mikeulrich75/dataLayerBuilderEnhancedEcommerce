___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const makeTableMap = require('makeTableMap');
const query = require('queryPermission');
const createQueue = require('createQueue');
const dataLayerPush = createQueue('dataLayer');
const log = require('logToConsole');
const merge = function() {
  const obj = {},
        il = arguments.length;
  let i = 0,
      key;
  for (; i < il; i++) {
    for (key in arguments[i]) {
      if (arguments[i].hasOwnProperty(key)) {
        obj[key] = arguments[i][key];
      }
    }
  }
  return obj;
};
const event = {'event': data.event};
const currencyParameter = data.currencyParameter;
const actionFieldParameters = data.actionFieldParameters ? makeTableMap(data.actionFieldParameters, 'name', 'value') : undefined;
const actionFieldDropDown = data.actionFieldDropDown;
const products = data.products;
const customParameter = data.customParameter ? makeTableMap(data.customParameter, 'name', 'value') : undefined;
const customParameterDropDown = data.customParameterDropDown;
const eventType = data.eventType;
const dLayer = {};
if (eventType === 'impressions') {
  dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'impressions': products 
 };
} else if (eventType === 'click') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'click': { 
     'actionField': actionFieldParameters,
     'products': products
  } 
 }; 
} else if (eventType === 'detail') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'detail': {
     'actionField': actionFieldParameters, 
     'products':  products
  }
 };
} else if (eventType === 'add') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'add': { 
     'actionField': actionFieldParameters, 
     'products':  products
  }
 };
} else if (eventType === 'remove') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'remove': { 
     'actionField': actionFieldParameters, 
     'products':  products 
  }
 }; 
} else if (eventType === 'promoView') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'promoView': { 
     'actionField': actionFieldParameters, 
     'promotions':  products 
  }
 }; 
} else if (eventType === 'promoClick') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'promoClick': { 
     'promotions':  products 
  }
 }; 
} else if (eventType === 'checkout') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'checkout': { 
     'actionField': actionFieldParameters, 
     'products':  products 
  }
 }; 
} else if (eventType === 'checkout_option') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'checkout_option': { 
     'actionField': actionFieldParameters, 
     'products':  products 
  }
 }; 
} else if (eventType === 'purchase') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'purchase': { 
     'actionField': actionFieldParameters, // Transaction ID. Required for purchases and refunds.
     'products':  products // Name or ID is required.
  }
 };
} else if (eventType === 'refund') {
 dLayer.ecommerce = {
    'currencyCode': currencyParameter,
    'refund': { 
     'actionField': actionFieldParameters, // Transaction ID. Required for purchases and refunds.
     'products':  products // Product ID and quantity. Required for partial refunds.
  }
 };
}
const dataLayer = merge(event, customParameter, dLayer);
if (query('access_globals', 'readwrite', 'dataLayer')) {
  dataLayerPush(dataLayer);
  log("data:", dataLayer);
}
data.gtmOnSuccess();


